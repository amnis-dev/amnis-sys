{
  "name": "@amnis/sys",
  "version": "0.0.0",
  "description": "TypeScript system for enterprise-level web and mobile projects.",
  "keywords": [
    "javascript",
    "typescript",
    "stateful",
    "system",
    "test",
    "mocking",
    "authentication",
    "authorization",
    "authn",
    "authz",
    "crypto",
    "crud",
    "rest",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:amnis-dev/amnis-system.git"
  },
  "files": [
    "types",
    "dist"
  ],
  "type": "module",
  "types": "./types/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "pnpm build:types && pnpm build:dist",
    "build:dist": "vite build --mode production",
    "build:types": "tsc -p tsconfig.json",
    "build:libs": "pnpm --filter '@amnis/*...' build",
    "lint": "tsc --noEmit && eslint --ext .ts --ext .tsx \"./\"",
    "lint:libs": "pnpm --filter '@amnis/*...' lint",
    "test": "echo \"Error: no test specified\"",
    "test:libs": "pnpm --filter '@amnis/*...' test"
  },
  "author": "Eric Crowell",
  "license": "MIT",
  "devDependencies": {
    "@amnis/eslint-config-node": "workspace:^",
    "@amnis/mock": "workspace:^",
    "@amnis/state": "workspace:^",
    "@amnis/tsconfig-node": "workspace:^",
    "@reduxjs/toolkit": "https://github.com/amnis-dev/packages/raw/main/redux-toolkit-2.0.0-beta.0-patched.tgz",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/supertest": "^2.0.12",
    "ajv": "^8.12.0",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.45.0",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "msw": "^1.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux": "5.0.0-beta.0",
    "redux-thunk": "3.0.0-alpha.3",
    "reselect": "^4.1.8",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-json-schema-generator": "^1.2.0",
    "ts-node": "^10.9.1",
    "type-fest": "^4.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  }
}
